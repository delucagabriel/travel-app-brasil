{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "travel-support-brasil-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/travelSupportBrasil/TravelSupportBrasilWebPart.js",
          "manifest": "./src/webparts/travelSupportBrasil/TravelSupportBrasilWebPart.manifest.json"
        }
      ]
    },
    "bradesco-admin-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/bradescoAdmin/BradescoAdminWebPart.js",
          "manifest": "./src/webparts/bradescoAdmin/BradescoAdminWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "TravelSupportBrasilWebPartStrings": "lib/webparts/travelSupportBrasil/loc/{locale}.js",
    "BradescoAdminWebPartStrings": "lib/webparts/bradescoAdmin/loc/{locale}.js"
  }
}
